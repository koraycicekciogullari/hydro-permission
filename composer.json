{
  "name": "koraycicekciogullari/hydro-permission",
  "description": "A hydro permission",
  "license": "MIT",
  "keywords": [
    "hydro",
    "permission"
  ],
  "type": "library",
  "authors": [
    {
        "name": "Koray Çiçekçioğulları",
        "email": "k.cicekciogullari@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "^6.0|^7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Koraycicekciogullari\\HydroPermission\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Koraycicekciogullari\\HydroPermission\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Koraycicekciogullari\\HydroPermission\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
